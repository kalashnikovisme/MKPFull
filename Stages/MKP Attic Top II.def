[Info]
name = "MKP Attic Top II"
;---------------------------------------------
[Camera]
startx = 0
starty = 0
boundleft = -600
boundright = 600
boundhigh = -190
boundlow =  0
verticalfollow = .9
floortension = 3
tension = 60
;---------------------------------------------
[PlayerInfo]
p1startx = -70
p1starty = 0
p1startz = 0
p1facing = 1
;--- Player 2 ---
p2startx = 70
p2starty = 0
p2startz = 0
p2facing = -1
;--------------------------------------------
leftbound  = -1000 ;Left bound (x-movement)
rightbound =  1000 ;Right bound
topbound  =  0     ;Top bound (z-movement)
botbound  =  0     ;Bottom bound
;--------------------------------------------------------
[Scaling]
topz     = 0       ;Top z-coordinate for scaling
botz     = 50      ;Bottom z-coordinate for scaling
topscale = 1       ;Scale to use at top
botscale = 1.2     ;Scale to use at bottom
;--------------------------------------------------------
[Bound]
screenleft = 15
screenright = 15
;--------------------------------------------------------
[StageInfo]
zoffset =215
autoturn =1
resetBG=0
;--------------------------------------------------------
[Shadow]
intensity = 128
yscale = .1
;--------------------------------------------------------
[Reflection]
intensity = 0
;--------------------------------------------------------
[Music]
bgmusic = sound/Good (25).mp3
bgvolume = 255

;--------------------------------------------------------
[BGdef]
spr = stages/MKP Attic Top II.sff
debugbg = 0

[BG BackGround]
type  = normal
spriteno = 0,0
layerno = 0
start = -765,-245;-580,-245
delta = .5,0.5
mask = 0

[BG Wall]
type  = normal
spriteno = 1,0
layerno = 0
start = -600,-220
delta = .56,0.56
mask = 1


[BG Window Glare]
type  = normal
spriteno = 5,0
layerno = 0
start = -355,-15
delta = .56,0.56
mask = 1
trans = add

[BG lights]
type  = normal
spriteno = 3,0
layerno = 0
start = -250,-20
mask = 1


[BG LightGlare]
type  = normal
spriteno = 4,0
layerno = 0
start = -280,-60
mask = 1
trans = add

[BG Bridge]
type  = normal
spriteno = 2,0
layerno = 0
start = -600,-25
delta = .7,.7
mask = 1



